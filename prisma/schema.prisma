generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Translation {
  id               Int      @id @default(autoincrement())
  translationDef   String
  lang String
  word Word @relation(fields: [wordId], references: [id])
  wordId Int @unique
}

model Word {
  id        Int     @id @default(autoincrement())
  wordDef   String
  desc      String?
  translation Translation?
}
